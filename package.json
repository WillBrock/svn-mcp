{
  "name": "svn-mcp",
  "version": "1.0.4",
  "description": "MCP server providing SVN repository information for Claude Code and other AI assistants",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "svn-mcp": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "start": "node dist/index.js",
