  "devDependencies": {
    "@types/node": "^20.0.0",
    "release-it": "^19.0.5",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WillBrock/svn-mcp"
